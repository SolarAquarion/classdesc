. /usr/local/share/aegis/profile
aecp classdesc.ps
rm *~ *.aux *.dvi *.log *.blg *.toc *.lof *.ilg *.idx
latex classdesc
makeindex classdesc.idx
bibtex classdesc
latex classdesc
latex classdesc
dvips classdesc
TMP=/tmp/eco-doc$$
mkdir $TMP
aecp classdesc
rm -rf classdesc
latex2html classdesc
rm -rf $TMP
rm *~ *.aux *.dvi *.log *.blg *.toc *.lof *.ilg *.idx
# remove extraneous file from repository
aegis -list -terse project_files|grep doc/classdesc/|while read nm; do 
  if [ ! -f ../$nm ]; then 
    aecpu ../$nm
    aerm -no_whiteout ../$nm
    fi
  done
aenf classdesc
